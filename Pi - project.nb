Cell[TextData[{
 "I will use the following system of equations to compute approximation of \
\[Pi]: \n",
 Cell[BoxData[{
  RowBox[{
   RowBox[{
    RowBox[{"a", "[", "i", "]"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"a", "[", 
        RowBox[{"i", "-", "1"}], "]"}], "+", 
       RowBox[{"b", "[", 
        RowBox[{"i", "-", "1"}], "]"}]}], ")"}], "/", "2"}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"b", "[", "i", "]"}], " ", "=", " ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"a", "[", 
        RowBox[{"i", "-", "1"}], "]"}], "*", 
       RowBox[{"b", "[", 
        RowBox[{"i", "-", "1"}], "]"}]}], ")"}], "]"}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"c", "[", "i", "]"}], " ", "=", " ", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"a", "[", "i", "]"}], "2"], "-", 
     SuperscriptBox[
      RowBox[{"b", "[", "i", "]"}], "2"]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"s", "[", "i", "]"}], " ", "=", " ", 
    RowBox[{
     RowBox[{"s", "[", 
      RowBox[{"i", "-", "1"}], "]"}], "-", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["2", "i"], "*", 
       RowBox[{"c", "[", "i", "]"}]}], ")"}]}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"p", "[", "i", "]"}], " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"2", 
      SuperscriptBox[
       RowBox[{"a", "[", "i", "]"}], "2"]}], ")"}], "/", 
    RowBox[{"s", "[", "i", "]"}]}]}]}],
  CellChangeTimes->{{3.8227422753438606`*^9, 3.8227423016857395`*^9}, {
   3.8227423451938076`*^9, 3.822742401718649*^9}, {3.8230072264774847`*^9, 
   3.8230072900882764`*^9}, {3.8230073367220554`*^9, 
   3.8230076137648277`*^9}, {3.823121855616819*^9, 3.8231218639505286`*^9}},
  ExpressionUUID->"622f7535-9a55-49b4-be32-ab80bfda9fb3"]
}], "Text",
