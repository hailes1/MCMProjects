     RowBox[{"approx", "=", " ", 
      RowBox[{"1", "/", 
       RowBox[{"chudnovsky", "[", "n", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"RealDigits", "[", 
          RowBox[{"approx", ",", "10", ",", "1", ",", "i"}], "]"}], 
         "\[LeftDoubleBracket]", 
         RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"RealDigits", "[", 
          RowBox[{"Pi", ",", "10", ",", "1", ",", "i"}], "]"}], 
         "\[LeftDoubleBracket]", 
         RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ",", 
       RowBox[{"i", "--"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"-", "i"}], "-", "1"}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "the", " ", "number", " ", "of", " ", "correct", " ", "digits", " ", 
     "after", " ", "the", " ", "decimal", " ", "point"}], " ", "*)"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
