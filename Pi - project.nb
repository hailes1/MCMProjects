Cell[BoxData[
 RowBox[{
  RowBox[{"ramanujan", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sum", " ", "=", " ", "0"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"sum", " ", "+=", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"4", "i"}], ")"}], "!"}], ")"}], "*", 
           RowBox[{"(", 
            RowBox[{"1103", "+", 
             RowBox[{"26390", "i"}]}], ")"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"i", "!"}], ")"}], "4"], "*", 
           SuperscriptBox["396", 
            RowBox[{"4", "i"}]]}], ")"}]}]}], "\[IndentingNewLine]", ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"sum", "*", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "*", 
          RowBox[{"Sqrt", "[", "2", "]"}]}], ")"}], "/", "9801"}], 
       ")"}]}]}]}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "Multiplying", " ", "the", " ", "sum", " ", "with", " ", "the", " ", 
     "constant"}], " ", "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.82214921118267*^9, 3.822149226385914*^9}, {
   3.82300550310911*^9, 3.823005537994384*^9}, {3.823005573974187*^9, 
