Cell[BoxData[
 RowBox[{
  RowBox[{"correctDigits1", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"approx", ",", 
      RowBox[{"i", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"approx", "=", " ", 
      RowBox[{"1", "/", 
       RowBox[{"ramanujan", "[", "n", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"RealDigits", "[", 
          RowBox[{"approx", ",", "10", ",", "1", ",", "i"}], "]"}], 
         "\[LeftDoubleBracket]", 
         RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"RealDigits", "[", 
          RowBox[{"Pi", ",", "10", ",", "1", ",", "i"}], "]"}], 
         "\[LeftDoubleBracket]", 
         RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ",", 
       RowBox[{"i", "--"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"-", "i"}], "-", "1"}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "the", " ", "number", " ", "of", " ", "correct", " ", "digits", " ", 
     "after", " ", "the", " ", "decimal", " ", "point"}], " ", "*)"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.82214921118267*^9, 3.822149226385914*^9}, 
