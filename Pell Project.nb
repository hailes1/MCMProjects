   RowBox[{"generateSequence", "[", 
    RowBox[{"0", ",", "1", ",", "10000"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PellLucasSeq", " ", "=", " ", 
   RowBox[{"generateSequence", "[", 
    RowBox[{"2", ",", "2", ",", "10000"}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.823791693672448*^9, 3.8237917122539825`*^9}, {
  3.823793221449484*^9, 3.823793223631693*^9}},
 CellLabel->"In[21]:=",ExpressionUUID->"fbe8f7de-594d-40fd-8d95-edd384f1ed06"],

