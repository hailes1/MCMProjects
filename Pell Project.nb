Cell["Implementation - Theorem 2", "Section",
 CellChangeTimes->{{3.823794495772272*^9, 
  3.823794521655792*^9}},ExpressionUUID->"ca5966cd-63ec-4c41-8d40-\
215848b37b47"],

Cell["\<\
First thing I will do is very similar to the previous code chunk. We first \
declare a function that takes in a number n which will be the number of \
sequences I want to enter into my pell sequence function defined above. Then \
I will set the 2 sums in the formula, to 0 because as the Sum function from \
Mathematica runs the sum increases simultaneously. I will then calculate the \
right hand side of the equation and the left hand side of the equation and \
compare them with a boolean operator.\
\>", "Text",
 CellChangeTimes->{{3.8237945613900146`*^9, 3.8237946134897356`*^9}, {
  3.823794728974245*^9, 
  3.8237947774876804`*^9}},ExpressionUUID->"f9232635-7ac3-4602-aa41-\
3c0c2fa1aca7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"verifyTheor2", "[", 
   RowBox[{"n_", ",", "m_", ",", "k_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"lhs", ",", "rhs"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"lhs", "=", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"p", "[", 
           RowBox[{"2", " ", "k"}], "]"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "*", "m"}], "+", "1"}], ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
     RowBox[{"rhs", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", 
         RowBox[{"(", 
          RowBox[{"2", "^", 
           RowBox[{"(", 
            RowBox[{"3", " ", "m"}], ")"}]}], ")"}]}], ")"}], "*", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"-", "1"}], ")"}], "^", "j"}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"q", "[", 
              RowBox[{
               RowBox[{"2", "*", "m"}], "+", "1", "-", 
               RowBox[{"2", "*", "j"}]}], "]"}], ")"}]}], ")"}], "*", 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"2", "*", "m"}], "+", "1"}], ",", "j"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"p", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"2", "*", "m"}], "+", "1", "-", 
                RowBox[{"2", "*", "j"}]}], ")"}], "*", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"2", "*", "n"}], "+", "1"}], ")"}]}], "]"}], "-", 
            RowBox[{"p", "[", 
             RowBox[{
              RowBox[{"2", "*", "m"}], "+", "1", "-", 
              RowBox[{"2", "*", "j"}]}], "]"}]}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "0", ",", "m"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"lhs", "\[Equal]", "rhs"}]}]}], " ", "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.823774618721183*^9, 3.8237746359198837`*^9}, {
